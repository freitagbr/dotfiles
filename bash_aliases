# .bash_aliases
# Brandon Freitag

alias ..='cd ..'
alias la='ls -lA'
alias ll='ls -lhACF'
alias c='clear'
alias cl='clear'
alias mk='mkdir -p'
alias rm='trash'
alias rmdir='rm -rf'
alias wp='webkit2png'
alias mip='dig +short myip.opendns.com @resolver1.opendns.com'
alias electron='./node_modules/.bin/electron .'
alias fpi='nmap -p 22 --open -sV 192.168.0.*'

